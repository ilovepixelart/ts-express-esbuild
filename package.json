{
  "name": "ts-express-esbuild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "etsc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r esbuild-register src/bin/www.ts",
    "biome": "npx @biomejs/biome check",
    "biome:fix": "npx @biomejs/biome check --write .",
    "test": "vitest run --coverage",
    "test:open": "npm test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "cookie-parser": "1.4.7",
    "debug": "4.4.1",
    "dotenv": "16.5.0",
    "express": "5.1.0",
    "http-errors": "2.0.0",
    "mongoose": "8.15.1",
    "morgan": "1.10.0",
    "pug": "3.0.3",
    "ts-cache-mongoose": "1.7.3",
    "ts-migrate-mongoose": "4.0.8",
    "ts-patch-mongoose": "2.9.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.8",
    "@types/debug": "4.1.12",
    "@types/express": "5.0.2",
    "@types/http-errors": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "22.15.21",
    "@vitest/coverage-v8": "3.1.4",
    "esbuild": "0.25.4",
    "esbuild-node-tsc": "2.0.5",
    "esbuild-register": "3.6.0",
    "mongodb-memory-server": "10.1.4",
    "nodemon": "3.1.10",
    "open-cli": "8.0.0",
    "typescript": "5.8.3",
    "vitest": "3.1.4"
  }
}
