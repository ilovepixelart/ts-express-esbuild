{
  "name": "ts-express-esbuild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "etsc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r esbuild-register src/bin/www.ts",
    "biome": "npx @biomejs/biome check",
    "biome:fix": "npx @biomejs/biome check --write .",
    "test": "vitest",
    "test:open": "vitest run --coverage"
  },
  "dependencies": {
    "@biomejs/biome": "1.9.4",
    "cookie-parser": "1.4.7",
    "debug": "4.3.7",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "http-errors": "2.0.0",
    "mongoose": "8.8.1",
    "morgan": "1.10.0",
    "pug": "3.0.3",
    "ts-cache-mongoose": "1.6.6",
    "ts-migrate-mongoose": "3.8.5",
    "ts-patch-mongoose": "2.6.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.7",
    "@types/debug": "4.1.12",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "22",
    "esbuild": "0.24.0",
    "esbuild-node-tsc": "2.0.5",
    "esbuild-register": "3.6.0",
    "merge": "2.1.1",
    "mongodb-memory-server": "10.1.2",
    "nodemon": "3.1.7",
    "open-cli": "8.0.0",
    "typescript": "5.6.3",
    "vitest": "^2.1.5"
  }
}
