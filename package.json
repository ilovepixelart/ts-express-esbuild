{
  "name": "ts-express-esbuild",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "build": "etsc src -d dist",
    "build:ts": "tsc --project tsconfig.json",
    "dev": "nodemon --ext ts --exec node --inspect -r esbuild-register src/bin/www.ts",
    "lint": "yarn eslint ./src --fix . --ext .js,.ts",
    "lint:fix": "yarn eslint ./src --fix . --ext .js,.ts --fix",
    "test": "jest --config jest.config.ts --detectOpenHandles --coverage",
    "test:open": "npm run test && open-cli coverage/lcov-report/index.html"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "http-errors": "2.0.0",
    "mongoose": "7.3.1",
    "morgan": "1.10.0",
    "pug": "3.0.2",
    "ts-cache-mongoose": "1.0.5",
    "ts-migrate-mongoose": "3.1.8",
    "ts-patch-mongoose": "2.0.7"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "4.1.7",
    "@types/cookie-parser": "1.4.3",
    "@types/debug": "4.1.8",
    "@types/express": "4.17.17",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.3",
    "@types/morgan": "1.9.4",
    "@types/node": "18",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "esbuild": "0.18.11",
    "esbuild-jest": "^0.5.0",
    "esbuild-node-tsc": "^2.0.5",
    "esbuild-register": "^3.4.2",
    "eslint": "8.44.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "29.6.1",
    "merge": "2.1.1",
    "nodemon": "^2.0.22",
    "open-cli": "7.2.0",
    "typescript": "5.0.4"
  }
}
